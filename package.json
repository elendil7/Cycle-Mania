{
  "name": "cycle-mania",
  "version": "1.0.5",
  "description": "A custom bot built for the Cycle Mania community, to communicate with Strava and do other cool things.",
  "main": "./src/Index.ts",
  "scripts": {
    "nodemon": "nodemon ./src/Index.ts --watch ./src --ignore ./src/Discord/Commands",
    "build": "tsc",
    "start": "node ./dist/src/Index.js",
    "pm2:start": "pm2 start ./dist/src/Index.js --name cycle-mania",
    "pm2:stop": "pm2 stop cycle-mania",
    "pm2:restart": "pm2 restart cycle-mania --update-env",
    "pm2:log": "pm2 log --lines 200"
  },
  "keywords": [],
  "author": "Vitalijs Strelcuks",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/node": "^20.4.4",
    "axios": "^1.4.0",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "node-html-parser": "^6.1.5",
    "pm2": "^5.3.0",
    "puppeteer": "^20.8.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-adblocker": "^2.13.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "undici": "^5.22.1"
  },
  "prettier": "./.prettierrc.yaml"
}
